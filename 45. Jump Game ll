int jump(int* nums, int numsSize) {
    int jumps=0;
    int fardis=0;
    int currentend=0;
    for(int i=0;i<numsSize-1;i++){
        if(i+nums[i]>fardis)
        fardis=i+nums[i];

        if(i==currentend){
            jumps++;
            currentend=fardis;

            if(currentend>numsSize-1)
            break;
        }
    }
    return jumps;
}
